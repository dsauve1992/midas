{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:zip": "zip -r app.zip * .[^.]* -x \"**node_modules**\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "docker compose up -d && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:init": "npx ts-node ./scripts/initDatabase.ts",
    "db:migrate-latest": "NODE_ENV=production knex migrate:latest",
    "db:rollback-all": "NODE_ENV=production knex migrate:rollback --all"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.428.0",
    "@aws-sdk/lib-dynamodb": "^3.430.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.2.5",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.5",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^3.0.4",
    "@sentry/cli": "^2.36.2",
    "@sentry/nestjs": "^8.32.0",
    "@sentry/profiling-node": "^8.32.0",
    "@types/passport-jwt": "^3.0.13",
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "danfojs-node": "1.1.2",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "jest-when": "^3.7.0",
    "jwks-rsa": "^3.1.0",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "technicalindicators": "^3.1.0",
    "telegraf": "^4.16.3",
    "testcontainers": "^10.2.2",
    "user-agents": "^1.0.1444",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@sentry/types": "^8.32.0",
    "@testcontainers/postgresql": "^10.8.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jest-when": "^3.5.5",
    "@types/knex": "^0.16.1",
    "@types/node": "^20.3.1",
    "@types/pg": "^8.11.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "resolutions": {
    "@types/mime": "3.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "globalSetup": "<rootDir>/../test/config/globalSetup.ts",
    "globalTeardown": "<rootDir>/../test/config/globalTeardown.ts"
  }
}
